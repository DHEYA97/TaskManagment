{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "bootstrap-datepicker@1.10.0",
      "destination": "wwwroot/lib/bootstrap-datepicker/",
      "files": [
        "js/bootstrap-datepicker.min.js",
        "css/bootstrap-datepicker.min.css"
      ]
    },
    {
      "library": "tempus-dominus@6.7.13",
      "destination": "wwwroot/lib/tempus-dominus/",
      "files": [
        "js/tempus-dominus.min.js",
        "css/tempus-dominus.min.css",
        "locales/ar.min.js",
        "locales/ar-SA.min.js"
      ]
    },
    {
      "library": "microsoft-signalr@8.0.7",
      "destination": "wwwroot/assets/Plugins/microsoft-signalr/",
      "files": [
        "signalr.min.js"
      ]
    },
    {
      "library": "flatpickr@4.6.13",
      "destination": "wwwroot/assets/Plugins/flatpickr/",
      "files": [
        "flatpickr.min.js",
        "l10n/ar.js"
      ]
    },
    {
      "library": "moment.js@2.30.1",
      "destination": "wwwroot/assets/Plugins/moment.js/",
      "files": [
        "locale/ar.min.js"
      ]
    }
